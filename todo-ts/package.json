{
  "name": "todo-ts",
  "description": "app using ts 2.0 and vue 2.0",
  "author": "dyu",
  "private": true,
  "main": "nw/main.js",
  "inject_js_start": "nw/start.js",
  "webkit": {
    "plugin": false
  },
  "window": {
    "icon": "nw/icon.png"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --devtool source-map",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "app.d": "TARGET=app rollup -c -w",
    "app.b": "TARGET=app rollup -c --environment production",
    "ns.d": "node-sass app.scss dist/app.css -w",
    "ns.b": "node-sass app.scss dist/app.css && npm run cleancss",
    "cleancss": "cleancss -O2 removeDuplicateRules:on dist/app.css > dist/app.min.css && mv dist/app.min.css dist/app.css"
  },
  "dependencies": {
    "coreds": "1.4.0",
    "coreds-ui": "1.3.0",
    "pavilion": "^2.0.3",
    "v_": "2.1.10",
    "vue": "2.1.10",
    "vue-router": "2.1.3",
    "vuets": "1.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "cssnano": "^3.7.5",
    "esm": "^3.2.25",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "noop2": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-unique-selectors": "^2.0.2",
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-closure-compiler-js": "^1.0.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-svelte": "^2.0.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svelte": "^1.24.0",
    "ts-loader": "^2.2.1",
    "tsm": "^2.2.1",
    "typescript": "2.9.2",
    "url-loader": "^0.5.8",
    "uvu": "^0.5.3",
    "vue-loader": "9.9.5",
    "vue-template-compiler": "2.1.10",
    "webpack": "2.2.1",
    "webpack-closure-compiler": "2.0.2",
    "webpack-dev-server": "2.2.1"
  }
}
